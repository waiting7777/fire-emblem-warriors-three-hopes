// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model MS_Hero {
  id          Int     @id @default(autoincrement())
  name        String
  cost        Int
  power       Int
  abilities   String
  description String
  collectible Boolean
  category    String
  source      String
}

model TI_Character {
  id     Int    @id @default(autoincrement())
  name   String
  season String
}
